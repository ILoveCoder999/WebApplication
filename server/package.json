{
  "name": "stuff-happens-server",
  "version": "1.0.0",
  "description": "Stuff Happens Card Game Server with Passport.js Authentication",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "echo 'No build step required for server'",
    "test": "echo 'Tests not implemented yet'",
    "db:reset": "rm -f db/stuff.db && node server.js",
    "db:backup": "cp db/stuff.db db/stuff.db.backup",
    "lint": "eslint .",
    "format": "prettier --write ."
